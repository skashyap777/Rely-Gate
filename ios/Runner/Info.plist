<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>RUDRA</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>RUDRA</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	
	<!-- Location permissions for Google Maps and Geolocator -->
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>This app needs location access to detect potholes at your current location.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>This app needs location access to detect potholes and report their exact location.</string>
	
	<!-- Camera permission for pothole detection -->
	<key>NSCameraUsageDescription</key>
	<string>This app needs camera access to take photos of potholes for detection and reporting.</string>

	<!-- Photo library permission for image picker -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>This app needs access to your photo library to select images for pothole reporting.</string>

	<!-- Microphone permission (if needed for any feature) -->
	<key>NSMicrophoneUsageDescription</key>
	<string>This app needs microphone access for certain features.</string>

	<!-- Push notifications permission for Firebase -->
	<key>NSUserNotificationsUsageDescription</key>
	<string>This app needs notification permissions to send you updates about pothole reports.</string>

	<!-- Google Maps API Key -->
	<key>GMSServices</key>
	<string>AIzaSyBU8zniWDcPMAUWkqIJ0iTmGbkF7jtRwzA</string>
	<key>io.flutter.embedded_views_preview</key>
	<true/>

	<!-- App Transport Security for API domain -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>pwd.mockify.org</key>
			<dict>
				<key>NSIncludesSubdomains</key>
				<true/>
				<key>NSTemporaryExceptionAllowsInsecureHTTPLoads</key>
				<false/>
				<key>NSTemporaryExceptionMinimumTLSVersion</key>
				<string>TLSv1.2</string>
				<key>NSTemporaryExceptionRequiresForwardSecrecy</key>
				<false/>
			</dict>
		</dict>
	</dict>

</dict>
